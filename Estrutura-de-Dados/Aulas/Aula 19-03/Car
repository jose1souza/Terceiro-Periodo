package pkg19032025;


/**
 *
 * @author 14679274654
 */
public class Car {
    private int velocidade;

    public int velocidadeAtual() {
        return this.velocidade;
    }

    public void alteraVelocidade(int novaVelocidade) {
        this.velocidade = novaVelocidade;
    }
}
